					$('#xq #newnumber').html(data.newnumber);
					$('#xq #platform').html(data.platform);
					$('#xq #remark').html(data.remark);
					//取消
					$('#tb_user_xq_dialog_btc').on('click',function(){
						$('#xq').dialog('close');
						window.location.reload();//重新载入当前页面
					});
					$('#xq').dialog({onClose:function(){
				  			 //你想在关闭时执行的代码
						window.location.reload();//重新载入当前页面
					}});
			 },
		});
	}
</script>
	   
	  
